{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "public/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon_grey.ico"
    },
    {
      "src": "/favicon_grey.ico",
      "dest": "/public/favicon_grey.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_URL": "$DATABASE_URL",
    "DIRECT_URL": "$DIRECT_URL",
    "JWT_SECRET": "$JWT_SECRET"
  }
}