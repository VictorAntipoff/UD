{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SUPABASE_URL": "$SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY": "$SUPABASE_SERVICE_ROLE_KEY",
    "JWT_SECRET": "$JWT_SECRET",
    "FRONTEND_URL": "https://ud-frontend-snowy.vercel.app"
  }
}